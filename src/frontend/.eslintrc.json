{
	"env": {
    "browser": true,
    "es6": true
  },
	"parserOptions": {
    "parser": "babel-eslint"
  },
	"extends": ["plugin:vue/essential"],
  "rules": {
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
    ]
  }
}
